        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL.admin_option"
        mc:Ignorable="d"
        Title="ProductOrOrder_AdminWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/admin option/5bcac630-8e8f-4262-9938-83ef4947ad98.jpg"/>
